:"202210130","season_type":"1","season":"2022","week":"1","date":"2022-09-13 00:15:00","away_team":"DEN","home_team":"SEA","channel":"ESPN","point_spread":"6.00","over_under":"43.00","stadium_id":"31","canceled":"0","forecast_temp_low":null,"forecast_temp_high":null,"forecast_description":null,"forecast_wind_chill":null,"forecast_wind_speed":null,"away_team_money_line":"-260","home_team_money_line":"213","global_game_id":"18021","global_away_team_id":"10","global_home_team_id":"30","score_id":"18021","status":"Scheduled","created_at":null,"updated_at":null}};
        var current_week = 1;

        var is_footclan = false;
                var is_admin_user = false;
                var is_writer_user = false;
        
        var posLimits = {
            QB: 35,
            RB: 100,
            WR: 125,
            TE: 50, 
        }

        var scoring = window.udk.selectedScoringSystem;
        var playersOnRoster = [];

        function getRankings(scoring) {
            playersOnRoster = [];
            if (window.udk.scoringSystems[scoring].hasOwnProperty('roster')) {
                var roster = window.udk.scoringSystems[scoring]['roster'];
                roster.forEach(function(player) {
                    playersOnRoster.push(player[0]);
                });
            }

            var udk = window.udk.getRankings();
            var rankings = udk.getAnalystRankings(position);

            for (var i = 0; i < rankings.length; i++) {

                if (!udk.projections.has(rankings[i].player_id)) {
                    continue;
                }

                var rankingsRow = udk.getRankingsRow(rankings[i].player_id, rankings[i].fantasy_position);

                var headshot;
                var headshot_url;
                if (rankings[i].headshot) {
                    headshot = '<img class="lazy lazy-hidden" src="//s26212.pcdn.co/wp-content/plugins/a3-lazy-load/assets/images/lazy_placeholder.gif" data-lazy-type="image" data-src="https://cloud.thefantasyfootballers.com/images/app-square-sm/' + rankings[i].headshot + '" alt="' + rankings[i].name + '" /><noscript><img src="https://cloud.thefantasyfootballers.com/images/app-square-sm/' + rankings[i].headshot + '" alt="' + rankings[i].name + '" /></noscript>';
                    headshot_url = 'https://cloud.thefantasyfootballers.com/images/app-square-sm/' + rankings[i].headshot;
                } else {
                    headshot = '<svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#player-placeholder"></use></svg>';
                }

                var trajectory = '';
                if (rankingsRow.trajectory > 0) {
                    trajectory = '<i class="player-trajectory up"></i>';
                } else if (rankingsRow.trajectory < 0) {
                    trajectory = '<i class="player-trajectory down"></i>';
                }

                var playerMarkup = rankings[i].name;
                if (rankings[i].slug) {
                    var playerProfileURL = '/fantasy/' + rankings[i].slug + '/';
                    playerMarkup = '<a href="' + playerProfileURL + '">' + rankings[i].name + '</a>';
                }

                rankings[i].opponent = '';
                if (games.hasOwnProperty(rankings[i].team)) {
                    var game = games[rankings[i].team];
                    if (game.home_team == rankings[i].team) {
                        rankings[i].opponent = 'vs ' + game.away_team;
                    } else {
                        rankings[i].opponent = '@ ' + game.home_team;
                    }
                } else {
                    rankings[i].opponent = 'N/A';
                }

                var playerTeam = 'FA';
                if (rankings[i].team) {
                    playerTeam = rankings[i].team;
                }

                var linetwo = '';
                if (position == 'FLEX') {
                    linetwo = rankings[i].fantasy_position + ' ' + rankings[i].team + ' (' + rankings[i].opponent + ')';
                } else {
                    linetwo = playerTeam + ' (' + rankings[i].opponent + ')';
                }

                if (rankings[i].injury_status) {
                    var injuredBodyPart = '';
                    if (rankings[i].injury_body_part) {
                        injuredBodyPart = ' with ' + rankings[i].injury_body_part;
                    }
                    linetwo += '<span class="blurb-tag injury">';
                    linetwo += '  <span class="with-tooltip">';
                    linetwo += '    <svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#udk-injury-report"></use></svg>';
                    linetwo += '    <span class="ffb-tooltip">' + rankings[i].injury_status + injuredBodyPart + '</span>';
                    linetwo += '  </span>';
                    linetwo += '</span>';
                }

                rankings[i].player = '';
                rankings[i].player += '<div class="player-container">';
                rankings[i].player += '  <div class="player-portrait">';
                rankings[i].player += '    ' + headshot;
                rankings[i].player += '  </div>';
                rankings[i].player += '  <div class="player-right">';
                rankings[i].player += '    <div class="player-right-line-one">';
                rankings[i].player += '      <div class="player-name">';
                rankings[i].player += '          ' + playerMarkup;
                if (trajectory) {
                    rankings[i].player += '      ' + trajectory;
                }
                rankings[i].player += '      </div>';
                rankings[i].player += '    </div>';
                rankings[i].player += '    <div class="player-right-line-two">';
                rankings[i].player += '      ' + linetwo;
                rankings[i].player += '    </div>';
                rankings[i].player += '  </div>';
                rankings[i].player += '</div>';

                rankings[i].score = rankingsRow.score;
                rankings[i].avg_rank = rankings[i].andy_rank + rankings[i].jason_rank + rankings[i].mike_rank;

                rankings[i].score_display = '-';
                if (rankings[i].score) {
                    rankings[i].score_display = parseFloat(Number(rankings[i].score).toFixed(1));
                }

                rankings[i].score_empty = '';
                rankings[i].score_empty += '<span class="with-tooltip footclan">';
                rankings[i].score_empty += '  <svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#lock"></use></svg>';
                rankings[i].score_empty += '  <div class="ffb-tooltip">';
                rankings[i].score_empty += '    <div class="snipe"><svg class="ffb white"><use xlink:href="' + ajax_object.svg_url + '#footclan"></use></svg></div>';
                rankings[i].score_empty += '    <p>Weekly Projected Points are available to #FootClan members. <a href="https://www.patreon.com/bePatron?u=1017400" target="_blank">Join to Unlock</a>.</p>';
                rankings[i].score_empty += '  </div>';
                rankings[i].score_empty += '</span>';

                rankings[i].player_video = '';
                if (rankings[i].tagged) {
                    rankings[i].player_video += '<button class="ffb-video--link" data-id="' + rankings[i].player_id + '" data-name="' + rankings[i].name + '" data-photo="' + headshot_url + '" aria-label="Recent video clips for ' + rankings[i].name + '">';
                    rankings[i].player_video += '  <svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#youtube"></use></svg>';
                    rankings[i].player_video += '</button>';
                } else {
                    rankings[i].player_video += '<span class="ffb-video--link--inactive">';
                    rankings[i].player_video += '  <span class="with-tooltip">';
                    rankings[i].player_video += '    <svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#youtube"></use></svg>';
                    rankings[i].player_video += '    <div class="ffb-tooltip">No Recent Clips Available for ' + rankings[i].name + '</div>';
                    rankings[i].player_video += '  </span>';
                    rankings[i].player_video += '</span>';
                }

            }

            rankings.sort(function (a, b) {
                var n = b.score - a.score;
                if (n !== 0) {
                    return n;
                }

                return a.avg_rank - b.avg_rank;
            });

            for (var i = 0; i < rankings.length; i++) {
                rankings[i].consensus = i + 1;
            }

            relevantRankings = rankings.slice(0, posLimits[position]);

            return relevantRankings;
        }

        var rankings = getRankings(scoring);

        var columns = [
            {
                data: 'player',
                title: 'Player',
                orderable: false,
                className: 'all name'
            },
            {
                data: 'name',
                title: 'Name',
                visible: false,
                className: 'export'
            },
            {
                data: 'team',
                title: 'Team',
                visible: false,
                className: 'export'
            },
        ];

        if (is_footclan || is_admin_user || is_writer_user) {
            columns.push(
                {
                    data: 'score_display',
                    title: ffbTable.titleWithTooltip('Proj. Pts', 'Projected Points for Week ' + current_week),
                    orderable: false,
                    searchable: false,
                    className: 'export all',
                },
            );
        } else {
            columns.push(
                {
                    data: 'score_empty',
                    title: ffbTable.titleWithTooltip('Proj. Pts', 'Projected Points for Week ' + current_week),
                    orderable: false,
                    searchable: false,
                    className: 'locked all',
                }
            );
        }

        columns.push(
            {
                data: 'consensus',
                title: ffbTable.titleWithTooltip('Rank', 'The Fantasy Footballers Consensus Rank for Week ' + current_week),
                searchable: false,
                className: 'export rank--cons all',
            },
            {
                data: 'andy_rank',
                title: 'Andy',
                searchable: false,
                className: 'export rank--ind andy desktop tablet-l tablet-p',
            },
            {
                data: 'jason_rank',
                title: 'Jason',
                searchable: false,
                className: 'export rank--ind jason desktop tablet-l tablet-p',
            },
            {
                data: 'mike_rank',
                title: 'Mike',
                searchable: false,
                className: 'export rank--ind mike desktop tablet-l tablet-p',
            },
            {
                data: 'player_video',
                title: 'Clips',
                searchable: false,
                orderable: false,
                className: 'all',
            },
        );

        var datatable = jQuery('#free-rankings-table').DataTable({
            data: rankings,
            paging: true,
            pageLength: 200,
            lengthChange: false,
            columns: columns,
            order: [[4, 'asc']],
            dom: 'lfBrt',
            responsive: true,
            buttons: [
                {
                    extend: 'copy',
                    exportOptions: {
                        columns: '.export'
                    },
                    text: '<svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#copy"></use></svg>Copy',
                    attr: {
                        'aria-label': 'Copy',
                    },
                },
                {
                    extend: 'csv',
                    exportOptions: {
                        columns: '.export'
                    },
                    text: '<svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#download"></use></svg>Download CSV',
                    attr: {
                        'aria-label': 'Download CSV',
                    },
                },
                {
                    extend: 'print',
                    exportOptions: {
                        columns: '.export'
                    },
                    text: '<svg class="ffb"><use xlink:href="' + ajax_object.svg_url + '#print"></use></svg>Print',
                    attr: {
                        'aria-label': 'Print',
                    },
                }
            ],
            autoWidth: false,
            createdRow: function(row, data, dataIndex) {
                if (playersOnRoster.includes(data.player_id)) {
                    jQuery(row).addClass('rostered');
                }
            },
            initComplete: function(settings, json) {
                ffbTable.tableAddBallersFaces();
                ffbTable.tableTooltipSetup();
            },
        });

        // Configure Export Buttons
        datatable.buttons(0, null).container().prependTo('#more .ffb-filters--modal');

        // On scoring select change
        jQuery(document).on('scoringSystemChanged', function() {
            scoring = window.udk.selectedScoringSystem;
            rankings = getRankings(scoring);

            datatable.clear();
            datatable.search('');
            datatable.rows.add(rankings);
            datatable.draw();
        });

        jQuery('#loading').hide();
        jQuery('.ffb-filters').show();

        function buttonWithTooltip(title, tooltip) {
            return ' <span class="with-tooltip">' + title + '<div class="ffb-tooltip">' + tooltip + '</div></span>';
        }

    });

</script>

<script src="https://apis.google.com/js/platform.js"></script>
</div>
				</div>
				</div>
						</div>
					</div>
		</div>
								</div>
					</div>
		</section>
									</div>
			</div>
					</div>
		                                            </section>
